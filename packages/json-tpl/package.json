{
  "name": "json-tpl",
  "version": "0.0.1",
  "description": "A simple JSON template engine",
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --noEmit false --outDir dist/cjs  --module commonjs --declaration false --declarationMap false --downlevelIteration true --target es5",
    "test": "jest"
  },
  "dependencies": {
    "tslib": "1.x.x || 2.x.x"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  }
}
