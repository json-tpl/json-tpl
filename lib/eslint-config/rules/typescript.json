{
  "plugins": ["@typescript-eslint"],
  "extends": ["plugin:@typescript-eslint/strict-type-checked"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "none",
        "ignoreRestSiblings": true
      }
    ]
  }
}
